		<key>Comment Pattern</key>
		<string><![CDATA[
		(?x:
			(\#.*?$)         |    (?# Line comment)
			(\%.*?$)              (?# Line comment)
		)
		]]></string>

		<key>Function Pattern</key>
		<string><![CDATA[
		(?x: 
			(?P<function>
				(
					(layout\s+)        |
					(scrap\s+)            |
					(centerline\s+) 
					)
				(?P<function_name>
					[a-zA-Z0-9_!:?@]+
				)
				(?s:.+?)
				\rend(layout|scrap|centerline)\b
			)
		)
		]]></string>

